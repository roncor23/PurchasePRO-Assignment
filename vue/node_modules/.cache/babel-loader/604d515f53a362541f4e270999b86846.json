{"ast":null,"code":"import ProductCard from '@/components/ProductCard.vue';\nimport CheckoutProduct from './CheckoutProduct.vue';\nimport CheckoutService from '@/services/CheckoutService';\nimport CatalogService from '@/services/CatalogService';\nimport UserService from '@/services/UserService';\nexport default {\n  name: 'ProductList',\n  components: {\n    ProductCard,\n    CheckoutProduct\n  },\n\n  data() {\n    return {\n      products: [],\n      showCheckout: false,\n      cart: [],\n      user: {\n        name: '',\n        email: ''\n      }\n    };\n  },\n\n  async created() {\n    try {\n      // Fetch products from the CatalogService\n      this.products = await CatalogService.getProducts();\n    } catch (error) {\n      console.error('Error loading products:', error.message); // Handle error as needed\n    }\n  },\n\n  methods: {\n    checkout(cart) {\n      console.log(\"Checkout\", cart);\n      this.cart = [...this.cart, ...cart]; // Merge current cart items with existing cart\n\n      this.showCheckout = true;\n    },\n\n    async placeOrder() {\n      try {\n        // Call the CheckoutService to place the order\n        await CheckoutService.placeOrder(this.cart); // Clear the cart after placing the order\n\n        this.cart = [];\n        this.showCheckout = false;\n        console.log('Order placed successfully!');\n      } catch (error) {\n        console.error('Error placing the order:', error.message); // Handle error as needed\n      }\n    },\n\n    async storeUserDetails() {\n      try {\n        // Call the storeUserDetails to store the user details\n        await UserService.storeUserDetails(this.user); // Clear the user after storing\n\n        this.user = {};\n        console.log('Store successfully!');\n      } catch (error) {\n        console.error('Error storing the user details:', error.message); // Handle error as needed\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA+BE,OAAOA,WAAP,MAAwB,8BAAxB;AACA,OAAOC,eAAP,MAA4B